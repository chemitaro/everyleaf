<h1>ログイン</h1>
<%= form_with(scope: :session, local: true, url: sessions_path, method: :post) do |f|%>
  <table>
    <tr>
      <th><%= f.label :email, "メールアドレス" %></th>
      <td><%= f.email_field :email %></td>
    </tr>
    <tr>
      <th><%= f.label :password, "パスワード" %></th>
      <td><%= f.password_field :password %></td>
    </tr>
  </table>
  <%= f.submit "ログイン" %>
<% end %>